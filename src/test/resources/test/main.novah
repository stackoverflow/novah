module main

import novah.test
import array (arrayTests)
import computation (computationTests)
import linkedList (linkedListTests)
import list (listTests)
import map (mapTests)
import math (mathTests)
import option (optionTests)
import result (resultTests)
import set (setTests)
import stream (streamTests)
import string (stringTests)

pub
main : Array String -> Unit
main _ =
  let tests = listTests
                ++ arrayTests
                ++ computationTests
                ++ linkedListTests
                ++ mapTests
                ++ mathTests
                ++ optionTests
                ++ resultTests
                ++ setTests
                ++ streamTests
                ++ stringTests
  case runTests tests of
    Success -> Java.exit 0
    Failure -> Java.exit 1
    Error -> Java.exit 2